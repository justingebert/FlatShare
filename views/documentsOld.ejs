<div>
    <div>Documents List</div> 
    <% documents.forEach(documents=> { %>
        <h5><%= documents.name %>
        <embed src="<%= documents.path %>" type="application/pdf" width="100%" height="600px" />
    <% }); %> 
</div>

<div>
    <form action="/documents" method="post">
        input name: <input type="text" name="name" />
        <!-- /pdf/<documentName>.pdf-->
        input path: <input type="text" name="path">
        <input type="submit" name="submit">
    </form>
</div> 