<h1>Expense Data for
    <%= expense.title %>
  </h1>
  <table class="table">

    <tr>
      <th>Title</th>
      <td>
        <%= expense.title %>
      </td>
    </tr>
    <tr>
      <th>Amount</th>
      <td>
        <%= expense.amount %>
      </td>
    </tr>
    <tr>
      <th>Paid by</th>
      <td>
        <%= expense.paidBy %>
      </td>
    </tr>
  </table> 
<div class="center">
  <a class="button" href="/expenses">View all expenses</a>
</div>
<div>
  <a href="<%=`/expenses/${expense._id}/edit`%>">Edit</a>
</div>
<div>
  <a href="<%= `/expenses/${expense._id}/delete` %>" onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
</div>