<h1>Expense Data for
    <%= expense.title %>
  </h1>
  <table class="table">

    <tr>
      <th>Title</th>
      <td>
        <%= expense.title %>
      </td>
    </tr>
    <tr>
      <th>Amount</th>
      <td>
        <%= expense.amount %>
      </td>
    </tr>
  </table>
  
  
<div class="center">
  <a class="button" href="/expenses">View all expenses</a>
</div>
  

<div>
  <a href="/expenses">View all expense</a>
</div>
<div>
  <a href="<%=`/expense/${expense._id}/edit`%>">Edit </a>
</div>
<div>
  <a href="<%= `/expenses/${expense._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
</div>